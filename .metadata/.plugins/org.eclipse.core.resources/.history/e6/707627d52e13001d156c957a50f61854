
public class MensagemJava {

	public static void main(String[] args) {
		//Criando variáveis em Java
		String usuario = "Cristhiane";
		
		//Mensagem que será exibida no console
		System.out.println("Seja bem vindo ao Mundo Java" + usuario + "!");
		System.out.println("Vamos Fazer um super desenvolvimento em Java.");

		
	}

}